<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.fisherevans.strfry"
    android:versionCode="11"
    android:versionName="2.5.1" >

    <uses-sdk android:minSdkVersion="8"  android:targetSdkVersion="13"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:screenOrientation="landscape"
            android:name=".StrFry"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sub_activities.Preferences" android:screenOrientation="landscape"></activity>
        <activity android:name=".sub_activities.HighScores" android:screenOrientation="landscape"></activity>
        <activity android:name=".sub_activities.Help" android:screenOrientation="landscape"></activity>
        <activity android:name=".sub_activities.About" android:screenOrientation="landscape"></activity>
    </application>

</manifest>